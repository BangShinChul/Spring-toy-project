<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bangshinchul.toy.SpringToyProject.Mapper">
	<!-- id는 DAO나 Controller 등 JAVA내에서 불러올 때 사용하는 id -->
	<!-- resultType은 mybatis-config.xml에서 정해준 typeAlias의 alias값으로 설정한다. -->
	<!-- 즉 mybatis-config.xml 에서 세팅한 alias의 type설정값=class경로에 맞게 결과를 리턴한다. -->
	<select id="test" resultType="Test">
		select * from test
	</select>
</mapper>